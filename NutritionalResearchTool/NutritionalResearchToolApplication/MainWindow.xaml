        mc:Ignorable="d"
        x:Name="mainWindows"
        Title="孕期食物频率调查系统" Height="650" Width="1032" Loaded="mainWindows_Loaded" MinWidth="1032" MaxWidth="1032" MinHeight="650" MaxHeight="650" WindowStartupLocation="CenterScreen">
